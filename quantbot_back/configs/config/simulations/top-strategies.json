{
  "name": "top-strategies-simulation",
  "description": "Run simulations with top 3 optimized strategies",
  "scenarios": [
    {
      "name": "Strategy_980",
      "tags": ["top-strategy", "optimized"],
      "data": {
        "kind": "file",
        "path": "data/exports/csv/all_brook_channels_calls.csv",
        "format": "csv",
        "mintField": "tokenAddress",
        "chainField": "chain",
        "timestampField": "timestamp",
        "startOffsetMinutes": 0,
        "durationHours": 1440,
        "filter": {}
      },
      "strategy": [
        { "target": 2.0, "percent": 0.1 },
        { "target": 3.0, "percent": 0.1 },
        { "target": 5.0, "percent": 0.1 }
      ],
      "stopLoss": {
        "initial": -0.5,
        "trailing": "none"
      },
      "outputs": [
        {
          "type": "csv",
          "path": "data/exports/csv/Strategy_980_trade_by_trade.csv",
          "append": false
        },
        {
          "type": "stdout",
          "detail": "summary"
        }
      ]
    },
    {
