{
  "name": "Example Strategy - RSI Signal Entry",
  "entry": {
    "mode": "signal",
    "signal": {
      "type": "rsi_below",
      "period": 14,
      "value": 30
    },
    "delay": {
      "mode": "none"
    }
  },
  "exits": {
    "targets": [
      {
        "size_pct": 100,
        "profit_pct": 15
      }
    ],
    "trailing": {
      "enabled": false
    },
    "time_exit": {
      "enabled": false
    }
  },
  "stops": {
    "stop_loss_pct": 10,
    "break_even_after_first_target": false
  },
  "execution": {
    "fill_model": "close",
    "fee_bps": 25,
    "slippage_bps": 10
  }
}

