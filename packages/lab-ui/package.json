{
  "name": "@quantbot/lab-ui",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc --build",
    "build:deps": "pnpm --filter @quantbot/core build && pnpm --filter @quantbot/utils build && pnpm --filter @quantbot/storage build",
    "prebuild": "pnpm build:deps",
    "start": "node --enable-source-maps dist/server.js"
  },
  "dependencies": {
    "@quantbot/cli": "workspace:*",
    "@quantbot/infra": "workspace:*",
    "ejs": "^3.1.10",
    "express": "^5.0.1",
    "nanoid": "^5.0.9"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.7",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
