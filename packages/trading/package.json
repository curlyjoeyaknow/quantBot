{
  "name": "@quantbot/trading",
  "version": "1.0.0",
  "description": "Live trading execution engine for QuantBot",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "vitest run tests/**/*.test.ts --exclude tests/integration/**",
    "test:watch": "vitest watch tests/**/*.test.ts --exclude tests/integration/**",
    "test:coverage": "vitest run --coverage --exclude tests/integration/**",
    "test:integration": "vitest run tests/integration/**/*.test.ts",
    "test:integration:watch": "vitest watch tests/integration/**/*.test.ts",
    "test:all": "vitest run"
  },
  "dependencies": {
    "@quantbot/utils": "workspace:*",
    "@quantbot/storage": "workspace:*",
    "@quantbot/simulation": "workspace:*",
    "@solana/web3.js": "^1.98.4",
    "@solana/spl-token": "^0.4.8",
    "@coral-xyz/anchor": "^0.32.1",
    "bs58": "^5.0.0",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}

