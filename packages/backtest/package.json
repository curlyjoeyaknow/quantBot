    "@quantbot/simulation": "workspace:*"
  },
  "devDependencies": {
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "duckdb": "^1.4.3",
    "execa": "^9.4.0",
    "type-fest": "^5.3.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
