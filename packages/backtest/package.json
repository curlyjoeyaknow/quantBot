{
  "name": "@quantbot/backtest",
  "version": "1.0.0",
  "description": "Minimum Viable Backtester - Golden path implementation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
