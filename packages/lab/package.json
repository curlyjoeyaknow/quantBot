{
  "name": "@quantbot/lab",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf dist .vitest coverage",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "server": "node --loader tsx/esm src/server.ts",
    "dev": "tsx watch src/server.ts"
  },
  "dependencies": {
    "@quantbot/core": "workspace:*",
    "@quantbot/infra": "workspace:*",
    "@quantbot/storage": "workspace:*",
    "@quantbot/utils": "workspace:*",
    "@quantbot/workflows": "workspace:*",
    "fastify": "^5.6.2",
    "luxon": "^3.5.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^2.1.9",
    "tsx": "^4.19.2",
    "typescript": "^5.9.2",
    "vitest": "^2.1.9"
  }
}
