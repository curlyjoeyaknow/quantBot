{
  "name": "@quantbot/cli",
  "version": "1.0.0",
  "description": "Unified CLI interface for all QuantBot packages",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "_comment": "No 'exports' field - CLI is an application, not a library. It should not be imported by other packages.",
  "bin": {
    "quantbot": "./dist/bin/quantbot.js"
  },
  "scripts": {
    "build": "tsc --build",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:mutation": "stryker run",
    "test:mutation:incremental": "stryker run --incremental",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "quality:check": "npm run format:check && npm run lint && npm run typecheck && npm test -- --run",
    "quality:full": "npm run quality:check && npm run test:coverage && npm run test:mutation",
    "cli": "node dist/bin/quantbot.js"
  },
  "dependencies": {
    "@quantbot/analytics": "workspace:*",
    "@quantbot/api-clients": "workspace:*",
    "@quantbot/core": "workspace:*",
    "@quantbot/data-observatory": "workspace:*",
    "@quantbot/ingestion": "workspace:*",
    "@quantbot/jobs": "workspace:*",
    "@quantbot/observability": "workspace:*",
    "@quantbot/ohlcv": "workspace:*",
    "@quantbot/simulation": "workspace:*",
    "@quantbot/storage": "workspace:*",
    "@quantbot/utils": "workspace:*",
    "@quantbot/workflows": "workspace:*",
    "@types/react": "^19.2.7",
    "bs58": "^5.0.0",
    "commander": "^12.0.0",
    "execa": "^9.0.0",
    "ink": "^6.5.1",
    "ink-select-input": "^6.2.0",
    "ink-text-input": "^6.0.0",
    "js-yaml": "^4.1.0",
    "luxon": "^3.4.4",
    "react": "^19.2.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^8.0.0",
    "@stryker-mutator/vitest-runner": "^8.0.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^24.10.1",
    "@vitest/coverage-v8": "^4.0.15",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  }
}
