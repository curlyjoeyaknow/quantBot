{
  "name": "quantbot",
  "version": "1.0.3",
  "description": "Advanced Telegram bot for trading simulation, CA monitoring, and PNL analysis using Birdeye API and Helius WebSockets",
  "main": "packages/workflows/src/index.ts",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "pnpm -r build",
    "build:packages": "pnpm --filter './packages/*' build",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist/ packages/*/dist/",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run --grep 'tests/unit'",
    "test:integration": "vitest run --grep 'tests/integration'",
    "test:properties": "vitest run --grep 'tests/properties'",
    "test:fuzzing": "vitest run --grep 'tests/fuzzing'",
    "test:e2e": "vitest run --grep 'tests/e2e'",
    "test:stress": "vitest run --config vitest.stress.config.ts",
    "test:ci": "vitest run --runInBand --coverage",
    "test:packages": "pnpm -r test",
    "lint": "eslint 'packages/**/src/**/*.ts' 'packages/**/tests/**/*.ts'",
    "lint:fix": "eslint 'packages/**/src/**/*.ts' 'packages/**/tests/**/*.ts' --fix",
    "lint:scripts": "eslint 'scripts/**/*.ts'",
    "lint:packages": "pnpm -r lint",
    "format": "prettier --write 'packages/**/src/**/*.ts' 'packages/**/tests/**/*.ts' 'scripts/**/*.ts' '*.json' '*.md'",
    "format:check": "prettier --check 'packages/**/src/**/*.ts' 'packages/**/tests/**/*.ts' 'scripts/**/*.ts' '*.json' '*.md'",
    "typecheck": "tsc --noEmit",
    "typecheck:packages": "pnpm -r typecheck",
    "precommit": "npm run format && npm run lint && npm run typecheck && npm test",
    "prepush": "npm run test:coverage && npm audit --audit-level=moderate",
    "check:test-requirements": "ts-node scripts/git/check-test-requirements.ts",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "analysis": "node scripts/analysis/historical_analysis.js",
    "extract": "node scripts/legacy/data-processing/extract_ca_drops.js",
    "simulate": "node scripts/legacy/simulation/simulate_accurate_final.js",
    "simulate:influxdb": "node scripts/legacy/simulation/simulate_original_strategy_influxdb.js",
    "dashboard": "node scripts/export_dashboard.js",
    "influxdb:start": "docker-compose up -d influxdb",
    "influxdb:stop": "docker-compose down",
    "influxdb:migrate": "node scripts/migration/migrate-csv-to-influx.js",
    "influxdb:test": "node scripts/test/test-influxdb-integration.js",
    "clickhouse:start": "docker-compose up -d clickhouse",
    "clickhouse:stop": "docker-compose stop clickhouse",
    "clickhouse:setup": "ts-node scripts/setup-clickhouse.ts",
    "clickhouse:migrate": "ts-node scripts/migrate-csv-to-clickhouse.ts",
    "monitor:credits": "node scripts/monitor-credits.js",
    "monitor:dashboard": "echo 'Monitoring package archived - see archive/scripts-monitoring/'",
    "monitor:bench": "echo 'Monitoring package archived - see archive/scripts-monitoring/'",
    "ohlcv:backfill": "npx ts-node --transpile-only scripts/ohlcv/run-backfill.ts",
    "caller:migrate": "node scripts/migration/migrate-ca-drops-to-caller-db-fixed.js",
    "caller:stats": "node scripts/migration/migrate-ca-drops-to-caller-db-fixed.js --stats",
    "simulate:caller": "node scripts/legacy/simulation/simulate-caller.js",
    "analyze:callers": "node scripts/analysis/analyze-callers.js",
    "extract:lsy": "node scripts/legacy/data-processing/extract-lsy-calls.js",
    "add:lsy": "node scripts/add-lsy-to-caller-db.js",
    "cleanup:bots": "node scripts/cleanup-bot-responses.js",
    "extract:all-brook": "node scripts/legacy/data-processing/extract-all-brook-channels.js",
    "extract:brook7": "ts-node scripts/extract-brook7-newest-calls.ts",
    "analyze:lsy": "node scripts/analysis/analyze-lsy-performance.js",
    "migrate:add-mcap": "ts-node scripts/migrate-db-add-mcap-column.ts",
    "backfill:mcap": "ts-node scripts/backfill-mcap-and-metadata.ts",
    "backfill:atl-ath": "ts-node scripts/backfill-atl-metrics.ts",
    "backfill:initial-price": "ts-node scripts/backfill-initial-price.ts",
    "optimize:strategies": "ts-node scripts/legacy/optimization/optimize-strategies.ts",
    "optimize:ml": "ts-node scripts/legacy/optimization/ml-strategy-optimizer.ts",
    "optimize:analyze": "ts-node scripts/legacy/analysis/analyze-strategy-results.ts",
    "optimize:all": "npm run optimize:strategies && npm run optimize:ml && npm run optimize:analyze",
    "fetch:tokens": "ts-node scripts/legacy/data-processing/fetch-all-tokens-to-clickhouse.ts",
    "simulate:config": "ts-node scripts/simulation/run-engine.ts",
    "analyze:brook": "ts-node scripts/analysis/analyze-brook-token-selection.ts",
    "score:tokens": "ts-node scripts/analysis/score-tokens-like-brook.ts",
    "create:unified-calls": "ts-node scripts/analysis/create-unified-calls-table.ts",
    "score:unified-calls": "ts-node scripts/analysis/score-and-analyze-unified-calls.ts",
    "view:results": "ts-node scripts/analysis/view-scored-results.ts",
    "view:results:html": "ts-node scripts/analysis/view-results-html.ts",
    "check:clickhouse": "ts-node scripts/analysis/check-clickhouse-data.ts",
    "validate:scoring": "ts-node scripts/analysis/validate-scoring-rolling-window.ts",
    "interpret:validation": "ts-node scripts/analysis/interpret-validation-results.ts",
    "monitor:brook": "echo 'Monitoring package archived - see archive/scripts-monitoring/'",
    "forward:brook": "echo 'Monitoring package archived - see archive/scripts-monitoring/'",
    "ingest:telegram": "ts-node scripts/ingest/telegram-alerts.ts",
    "ingest:telegram:json": "ts-node scripts/ingest/telegram-json.ts",
    "ingest:batch-aug-dec": "ts-node scripts/ingest/batch-telegram-aug-dec.ts",
    "ingest:ohlcv": "ts-node scripts/ingest/fetch-ohlcv-for-calls.ts",
    "simulate:calls": "ts-node scripts/simulation/run-strategy-on-calls.ts",
    "score:v2": "ts-node scripts/analysis/analyze-and-score-v2.ts",
    "deep-dive:brook": "ts-node scripts/analysis/deep-dive-brook-winners.ts",
    "cli": "node packages/cli/dist/bin/quantbot.js",
    "tui": "node packages/tui/dist/index.js",
    "prepare": "husky"
  },
  "keywords": [
    "telegram",
    "bot",
    "trading",
    "simulation",
    "birdeye",
    "helius",
    "solana",
    "crypto",
    "pnl",
    "ichimoku",
    "technical-analysis"
  ],
  "author": "QuantBot Team",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@clickhouse/client": "^1.14.0",
    "@coral-xyz/anchor": "^0.32.1",
    "@coral-xyz/borsh": "^0.32.1",
    "@influxdata/influxdb-client": "^1.33.2",
    "@influxdata/influxdb-client-apis": "^1.33.2",
    "@solana/web3.js": "^1.98.4",
    "@triton-one/yellowstone-grpc": "^4.0.2",
    "@types/sqlite3": "^3.1.11",
    "@types/ws": "^8.18.1",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "commander": "^12.0.0",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "lru-cache": "^10.0.0",
    "luxon": "^3.7.2",
    "minipass-json-stream": "^1.0.2",
    "pg": "^8.13.0",
    "sqlite3": "^5.1.7",
    "telegraf": "^4.16.3",
    "winston": "^3.18.3",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.10.0",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.49.0",
    "@typescript-eslint/parser": "^8.49.0",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "fast-check": "^4.4.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "node-gyp": "^12.1.0",
    "prettier": "^3.7.4",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.49.0",
    "vitest": "^4.0.15"
  }
}
