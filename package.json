{
  "name": "quantbot",
  "version": "1.0.0",
  "description": "Advanced Telegram bot for trading simulation, CA monitoring, and PNL analysis using Birdeye API and Helius WebSockets",
  "main": "src/bot/bot.ts",
  "scripts": {
    "start": "ts-node src/bot/bot.ts",
    "dev": "ts-node --watch src/bot/bot.ts",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist/",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "analysis": "node scripts/analysis/historical_analysis.js",
    "extract": "node scripts/data-processing/extract_ca_drops.js",
    "simulate": "node scripts/simulation/simulate_accurate_final.js",
    "dashboard": "node export_dashboard.js"
  },
  "keywords": [
    "telegram",
    "bot",
    "trading",
    "simulation",
    "birdeye",
    "helius",
    "solana",
    "crypto",
    "pnl",
    "ichimoku",
    "technical-analysis"
  ],
  "author": "QuantBot Team",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/sqlite3": "^3.1.11",
    "@types/ws": "^8.18.1",
    "axios": "^1.12.2",
    "cheerio": "^1.1.2",
    "csv-parse": "^6.1.0",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.3",
    "luxon": "^3.7.2",
    "sqlite3": "^5.1.7",
    "telegraf": "^4.16.3",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/luxon": "^3.7.1",
    "@types/node": "^24.9.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
